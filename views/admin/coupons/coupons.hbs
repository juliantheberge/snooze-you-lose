<div class="full-page">
  <h1>Cuopons</h1>
  <p>
    View, create, edit and delete coupons.
  </p>
  <form action="/admin/new-coupon" method="get">
    <button class="no" type="submit">new coupon</button>
  </form>
  <div class= "categoryListWrapper">
    {{#each couponContent}}
      {{> admin-coupondiv}}
    {{/each}}
  </div>
  <p class="formErrorReport">{{ dbError }}</p>
  <form action="/admin/home" method="get">
    <button class="no" type="submit">home</button>
  </form>

</div>
