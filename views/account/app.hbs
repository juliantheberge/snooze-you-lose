<div id='app'>
    <div class='app-wrapper'>
        <ul class="app-menu">
            <div class='app-menu-title-wrapper'>
                <img class='formIcon fadeIn' src='/icons/logo-placeholder.svg' />
                <h1 class='app-menu-title'>s.y.l.</h1>
            </div>
            <li class='app-menu-li'>
                <img class='formIcon fadeIn' src='/icons/white/clock-alarm.svg' />
                <a href="/app/accounts/{{email}}/alarms" class='app-menu-text'>alarms</a>
            </li>

            <li class='app-menu-li'>
                <img class='formIcon fadeIn' src='/icons/white/squares.svg' />
                <a href="/app/accounts/{{email}}/orgs" class='app-menu-text '>organizations</a>
            </li>

            <li class='app-menu-li'>
                <img class='formIcon fadeIn' src='/icons/white/graph-bar.svg' />
                <a href="/app/accounts/{{email}}/insights" class='app-menu-text-disabled disabled-a'>insights</a>
            </li>

            <li class='app-menu-li'>
                <img class='formIcon fadeIn' src='/icons/white/user-fem.svg' />
                <a href="/app/accounts/{{email}}/profile" class='app-menu-text-disabled disabled-a'>profile</a>
            </li>

            <li class='app-menu-li'>
                <img class='formIcon fadeIn' src='/icons/white/mixer.svg' />
                <a href="/app/accounts/{{email}}/settings" class='app-menu-text-disabled disabled-a'>settings</a>
            </li>

            <li class='app-menu-li' id='logout'>
                <img class='formIcon fadeIn' src='/icons/white/back-1.svg' />
                <form action="/log-out" method="post">
                    <button type="submit" class="app-menu-text">logout</button>
                </form>
            </li>

        </ul>
        <div class="app-content">
            {{!-- defaulting to alarm guest --}}
            <div class="full-page">
                <h1>Clock</h1>
                <p>
                    This is where you can view, add, remove and edit your alarms
                </p>
                <div class='flex space-around'>
                    <div class='flex center'>
                        <h2 id='clock'></h2>
                    </div>
                </div>
                <h1>My alarms</h1>
                <div class="categoryListWrapper">
                    {{#each alarmContent}} {{> alarmdiv}} {{/each}}
                </div>
                <p class="formErrorReport">{{ dbError }}</p>
                <form action="" method='post'>
                    <button class='yes'>finish month</button>
                </form>
                <form action="" method="get">
                    <button class="yes"> add alarm </button>
                </form>
                <form action="/home" method="get">
                    <button class="no" type="submit">back</button>
                </form>
            </div>
        </div>
    </div>
</div>
<script src='./rollup/bundle.js'></script>