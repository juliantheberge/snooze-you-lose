{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "jsEnable": true,
    "rules": {
        "no-consecutive-blank-lines": false,
        "no-console": {
            "severity": "warning",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace",
            ]
        },
        "import-source-order": {
            "named-imports-order": ["lowecase-last"]
        }
    },
    "rulesDirectory": []
}